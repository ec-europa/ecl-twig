{
  "private": true,
  "scripts": {
    "build:ec": "build-storybook -c ./src/ec/.storybook -o build/ec",
    "build:eu": "echo \"Nothing to do...\"",
    "build-storybook:ec": "build-storybook -c php/ec/.storybook -o php/dist/ec && yarn copy:php && yarn fix-php-dist",
    "build-storybook:eu": "echo \"Nothing to do...\"",
    "changelog": "lerna-changelog",
    "check:release": "scripts/check-packages.sh",
    "check:component": "run-s clean:php \"render:* {1} {2}\" pretty:php \"diff {1} {2}\" --",
    "copy:static": "ncp static build",
    "copy:php": "node_modules/@ecl-twig/php-storybook/resources/get-assets.js",
    "clean": "rimraf build dist",
    "clean:php": "rimraf build php/packages php/dist",
    "dist": "npm-run-all clean build:* copy:static",
    "dist:php": "npm-run-all clean:php render-php-js build-storybook:*",
    "fix-php-dist": "node_modules/@ecl-twig/php-storybook/resources/fix-build.js",
    "deploy:php": "gh-pages -d php/dist",
    "diff": "node_modules/@ecl-twig/php-storybook/scripts/diff.js",
    "ecl-diff": "node_modules/@ecl-twig/php-storybook/scripts/ecl-diff.js --interactive",
    "grump-dis": "./scripts/grump-dis.js",
    "grump-en": "./scripts/grump-en.js",
    "jest": "jest",
    "jest:update": "jest -u",
    "lint:js": "eslint .",
    "lint": "npm run lint:js",
    "postinstall": "lerna run postinstall && lerna run prepublish && patch-package",
    "pretty:check": "prettier --check \"src/**/*.{md,mdx,html,json,yml,js}\"",
    "pretty:fix": "prettier --write \"src/**/*.{md,mdx,html,json,yml,js}\"",
    "pretty:php": "yarn php-html-fix && yarn php-story-fix",
    "php-story-fix": "prettier --write \"php/**/*.story.js\"",
    "php-html-fix": "prettier --write \"php/packages/**/*.html\"",
    "publish": "scripts/publish.sh",
    "render:dataGen": "babel-node ./node_modules/@ecl-twig/php-storybook/scripts/generate-data-files.js",
    "setupPhp": "cd ./node_modules/@ecl-twig/php-storybook && composer install",
    "render:php": "cd ./node_modules/@ecl-twig/php-storybook && composer render",
    "render:js": "node_modules/@ecl-twig/php-storybook/scripts/render.js",
    "render-php-js": "run-s \"render:* {1} \" pretty:php --",
    "start:ec": "start-storybook -p 9001 -c src/ec/.storybook",
    "start:eu": "echo \"Nothing to do...\"",
    "start:php-ec": "npm-run-all setupPhp clean:php render-php-js storybook:php-ec",
    "start:php-eu": "echo \"Nothing to do...\"",
    "storybook:php-ec": "start-storybook -s ./static/images -p 9002 -c php/ec/.storybook",
    "storybook:php-eu": "start-storybook -s ./static/images -p 9002 -c php/eu/.storybook",
    "update-version": "lerna version --no-push --no-git-tag-version",
    "test": "npm run jest"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@ecl-twig/data-utils": "file:utils/data-utils",
    "@ecl-twig/php-storybook": "file:php/php_storybook",
    "@ecl-twig/story-utils": "file:utils/story-utils",
    "@ecl-twig/storybook-addon-code": "file:utils/storybook-addon-code",
    "@ecl-twig/storybook-addon-diff": "file:utils/storybook-addon-diff",
    "@ecl-twig/storybook-addon-jscode": "file:utils/storybook-addon-jscode",
    "@ecl-twig/storybook-addon-notes": "file:utils/storybook-addon-notes",
    "@ecl-twig/test-utils": "file:utils/test-utils",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-options": "5.3.18",
    "@storybook/addon-viewport": "5.3.18",
    "@storybook/html": "5.3.18",
    "acorn": "^6.4.1",
    "babel-jest": "24.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-preval": "4.0.0",
    "braces": "^2.3.1",
    "clipboard": "2.0.6",
    "core-js": "3.6.5",
    "decompress": "4.2.1",
    "deepmerge": "4.2.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-unicorn": "15.0.1",
    "file-system": "2.2.2",
    "gh-pages": "2.2.0",
    "html-differ": "1.4.0",
    "husky": "4.2.5",
    "jest": "24.9.0",
    "jq-cli-wrapper": "1.6.1",
    "js-beautify": "1.10.3",
    "kind-of": "^6.0.3",
    "lerna": "3.20.2",
    "lerna-changelog": "1.0.1",
    "lint-staged": "9.5.0",
    "marked": "0.8.0",
    "minimist": "^1.2.5",
    "ncp": "2.0.0",
    "netlify-cli": "2.29.0",
    "node-fetch": "2.6.0",
    "npm-run-all": "4.1.5",
    "patch-package": "6.2.2",
    "prettier": "1.19.1",
    "prismjs": "1.19.0",
    "puppeteer": "2.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "replace-in-file": "4.2.0",
    "rimraf": "3.0.2",
    "tunnel-agent": "^0.6.0",
    "twing": "2.3.7",
    "twing-loader": "2.0.2",
    "webpack": "4.42.1",
    "yargs-interactive": "3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,mdx,html,json,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "workspaces": [
    "src/ec/packages/*",
    "src/eu/packages/*",
    "src/php/packages/*"
  ],
  "jest": {
    "verbose": true
  },
  "dependencies": {
    "serialize-javascript": "3.0.0"
  },
  "resolutions": {
    "acorn": "^6.4.1",
    "braces": "^2.3.1",
    "tunnel-agent": "^0.6.0",
    "decompress": "4.2.1",
    "kind-of": "^6.0.3",
    "minimist": "^1.2.5",
    "istanbul-reports": "^3.0.0"
  }
}
