{
  "private": true,
  "scripts": {
    "changelog": "lerna-changelog",
    "check:release": "scripts/check-packages.sh",
    "check:component": "run-s \"php-clean:{1}\" \"render:* {1} {2}\" \"php-prettify:{1}\" \"diff {1} {2}\" --",
    "clean": "run-p twing:clean php:clean",
    "diff": "node_modules/@ecl-twig/php-storybook/scripts/diff.js",
    "diff:ecl": "node_modules/@ecl-twig/php-storybook/scripts/ecl-diff.js --interactive",
    "dist": "run-s clean twing:dist php:dist",
    "grump-dis": "./scripts/grump-dis.js",
    "grump-en": "./scripts/grump-en.js",
    "jest": "jest",
    "jest:update": "jest -u",
    "lint": "eslint .",
    "php:copy": "./node_modules/@ecl-twig/php-storybook/resources/get-assets.js",
    "php-clean:eu": "rimraf build php/packages/eu php/dist/eu",
    "php-clean:ec": "rimraf build php/packages/ec php/dist/ec",
    "php:dist": "npm-run-all php:clean render-php-js storybook-build-php:* php:copy php:dist-fix",
    "php:clean": "npm-run-all php-clean:*",
    "php:dist-fix": "./node_modules/@ecl-twig/php-storybook/resources/fix-build.js",
    "php:install": "lerna --scope \"@ecl-twig/php-storybook\" run setup",
    "php-prettify:ec": "prettier --write \"php/packages/ec/**/*.{html,js}\"",
    "php-prettify:eu": "prettier --write \"php/packages/eu/**/*.{html,js}\"",
    "postinstall": "lerna run postinstall && lerna run prepublish && patch-package",
    "pretty:check": "prettier --check \"src/**/*.{md,mdx,html,json,yml,js}\"",
    "pretty:fix": "prettier --write \"src/**/*.{md,mdx,html,json,yml,js}\"",
    "pretty:php": "run-p php-prettify:*",
    "publish": "scripts/publish.sh",
    "render:dataGen": "babel-node ./node_modules/@ecl-twig/php-storybook/scripts/generate-data-files.js",
    "render:php": "lerna --scope \"@ecl-twig/php-storybook\" run render-php --",
    "render:js": "lerna --scope \"@ecl-twig/php-storybook\" run render-js --",
    "render-php-js": "run-s \"render:* {1} \" pretty:php --",
    "start:ec": "yarn storybook-start-twing:ec",
    "start:eu": "yarn storybook-start-twing:eu",
    "start:php-ec": "npm-run-all php:install php-clean:ec \"render-php-js ec\" storybook-start-php:ec",
    "start:php-eu": "npm-run-all php:install php-clean:eu \"render-php-js eu\" storybook-start-php:eu",
    "storybook-build-twing:ec": "build-storybook -c ./src/ec/storybook/.storybook -o build/ec",
    "storybook-build-twing:eu": "STORYBOOK_SYSTEM=EU build-storybook -c ./src/eu/storybook/.storybook -o build/eu",
    "storybook-build-php:ec": "build-storybook -c php/ec/.storybook -o php/dist/ec",
    "storybook-build-php:eu": "STORYBOOK_SYSTEM=EU build-storybook -c php/eu/.storybook -o php/dist/eu",
    "storybook-start-twing:ec": "start-storybook -p 9001 -c src/ec/storybook/.storybook",
    "storybook-start-twing:eu": "STORYBOOK_SYSTEM=EU start-storybook -p 9002 -c src/eu/storybook/.storybook",
    "storybook-start-php:ec": "start-storybook -s ./static/images -p 9003 -c php/ec/.storybook",
    "storybook-start-php:eu": "STORYBOOK_SYSTEM=EU start-storybook -s ./static/images -p 9004 -c php/eu/.storybook",
    "test": "npm run jest",
    "twing:copy": "ncp static build",
    "twing:clean": "rimraf build dist",
    "twing:dist": "npm-run-all twing:clean storybook-build-twing:* twing:copy",
    "update-version": "lerna version --no-push --no-git-tag-version"
  },
  "devDependencies": {
    "@babel/core": "7.11.0",
    "@babel/node": "7.10.5",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "babel-jest": "26.2.1",
    "babel-loader": "8.1.0",
    "babel-plugin-preval": "5.0.0",
    "core-js": "3.6.5",
    "eslint": "7.5.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-unicorn": "21.0.0",
    "file-system": "2.2.2",
    "husky": "4.2.5",
    "jest": "26.2.1",
    "jq-cli-wrapper": "1.6.1",
    "js-beautify": "1.11.0",
    "lerna": "3.22.1",
    "lerna-changelog": "1.0.1",
    "lint-staged": "10.2.11",
    "ncp": "2.0.0",
    "netlify-cli": "2.58.0",
    "node-fetch": "2.6.0",
    "npm-run-all": "4.1.5",
    "patch-package": "6.2.2",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rimraf": "3.0.2",
    "twing": "2.3.7",
    "twing-loader": "2.0.2",
    "webpack": "4.44.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,mdx,html,json,yml}": [
      "prettier --write"
    ],
    "*.js": [
      "prettier --write",
      "eslint"
    ]
  },
  "workspaces": [
    "src/ec/packages/*",
    "src/eu/packages/*",
    "utils/*",
    "php/php_storybook",
    "src/ec/storybook",
    "src/eu/storybook"
  ],
  "jest": {
    "verbose": true
  },
  "resolutions": {
    "braces": "^3.0.2",
    "dot-prop": "^5.1.1",
    "tunnel-agent": "^0.6.0",
    "decompress": "^4.2.1 "
  }
}
